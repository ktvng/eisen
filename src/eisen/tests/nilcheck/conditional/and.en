struct obj {
    x: int

    create(x: int) -> self: new obj {
        self.x = x
    }
}

fn maybe(a: obj?) -> r: obj? {
    r = a
}

fn main() {
    let a = obj(7)
    var? p = maybe(a)
    var? q = maybe(a)
    if (p != nil and q != nil) {
        print("%i", p.x + q.x)
    }
}

/// [Test]
/// name = "nilcheck/conditional/and"
/// info = "Nil check narrows types for multiple variables"

/// [Expects]
/// success = true
/// output = "14"
