struct obj {
    x: int

    create(x: int) -> self: new obj {
        self.x = x
    }
}

fn main() {
    let a = obj(7)
    var? p = a
    print("%i", p.x)
}

/// [Test]
/// name = "nilcheck/provability"
/// info = "Nilable types which are provablely not nil can be used directly"

/// [Expects]
/// success = true
/// output = "7"
