struct abc {
    a: int

    create(n: int) -> self: new abc {
        self.a = n
    }
}
struct obj {
    val z: abc

    create(z: abc) -> self: new obj {
        self.z = z
        print("%i ", self.z.a)
    }
}

fn main() {
    let z = abc(0)
    let a = abc(1)
    let o = obj(z)

    val v: abc
    v = a

    print("%i ", v.a)
    a.a = 41
    print("%i ", v.a)
}

