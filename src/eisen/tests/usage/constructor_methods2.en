struct obj {
    x: int
    y: int

    create(x: int, y: int) -> self: obj {
        self.x = x
        self.y = y
        self.init()
    }
}

fn init(self: obj) {
    self.x = 0
    self.y = 0
}

fn main() {
    let a = obj(3, 3)
    print("%i %i", a.x, a.y)
}

