fn main() {
    if (true) {
        print("1")
    }
    else {
        print("2")
    }

    if (false) {
        print("1")
    }
    else {
        print("2")
    }

    if (1 > 2) {
        print("1")
    }
    else if (2 > 2) {
        print("2")
    }
    else if (3 > 2) {
        print("3")
    }
    else {
        print("4")
    }

    if (1 > 3) {
        print("1")
    }
    else if (2 > 3) {
        print("2")
    }
    else {
        print("4")
    }
}

