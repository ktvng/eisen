struct abc {
    a: int

    create(n: int) -> self: new abc {
        self.a = n
    }
}

struct obj {
    val z: abc

    create(x: int, z: abc, z2: abc) -> self: new obj {
        self.z = z
        self.z = z2
    }
}

fn main() {
    let z = abc(0)
    let a = abc(1)
    let o = obj(1, z, a)
    val v: abc
    v = a
    o.z.a = 4
    v.a = 5
}

