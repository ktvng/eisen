struct obj {
    x: int

    create(x: int) -> self: new obj {
        self.x = x
    }
}

fn maybe(a: obj?) -> r: obj? {
    r = a
}

fn main() {
    let a = obj(1)
    var? p = maybe(a)
    var? q = maybe(a)

    if (p != nil or q != nil) {
        print("%i", p.x + q.x)
    }
}

