struct obj {
    x: int

    create() -> self: new obj {
        self.x = 0
    }
}

fn main() {
    let o, d = obj(), obj()
    var p = o
    if (true) {
        p = d
        return
    }

    @compiler_assert(reference_has_dependencies, p, o)
}

