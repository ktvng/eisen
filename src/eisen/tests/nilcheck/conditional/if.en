struct obj {
    x: int

    create(x: int) -> self: new obj {
        self.x = x
    }
}

fn maybe(a: obj?) -> r: obj? {
    r = a
}

fn main() {
    let a = obj(7)
    var? p = maybe(a)
    if (p != nil) {
        print("%i", p.x)
    }
}

/// [Test]
/// name = "nilcheck/conditional/if"
/// info = "Nil check narrows a type"

/// [Expects]
/// success = true
/// output = "7"
