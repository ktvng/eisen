struct obj {
    x: int
    y: int

    create(x: int, y: int) -> self: new obj {
        self.x = x
        self.y = y
    }
}

fn main() {
    let x = int[]
    x.append(4)
    x.append(6)
    let y = obj[]
    let a = obj{1, 2}
    let b = obj{2, 3}
    y.append(a)
    y.append(b)

    print("%i %i %i %i", x[0], x[1], y[0].x, y[1].y)
}

