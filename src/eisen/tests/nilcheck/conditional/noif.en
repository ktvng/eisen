struct obj {
    x: int

    create(x: int) -> self: obj {
        self.x = x
    }
}

fn maybe(a: obj?) -> r: obj? {
    r = a
}

fn main() {
    let a = obj(7)
    var? p = maybe(a)
    print("%i", p.x)
}

