struct obj {
    x: int
    y: int

    create(x: int, y: int) -> self: new obj {
        self.x = x
        self.y = y
    }
}

fn main() {
    let a = obj(3, 3)
    var? p = a
    var? q = a
    p = nil
    if (p != nil and q != nil) {
        print("%i", p.x + q.x)
    }

    let b = obj(4, 4)
    p = b
    if (q != nil and p != nil) {
        print("%i", p.x + q.x)
    }
}

