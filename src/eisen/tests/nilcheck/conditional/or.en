struct obj {
    x: int

    create(x: int) -> self: obj {
        self.x = x
    }
}

fn main() {
    let a = obj(1)
    var? p = a
    var? q = a
    if (p != nil or q != nil) {
        print("%i", p.x + q.x)
    }
}

