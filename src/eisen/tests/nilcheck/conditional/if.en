struct obj {
    x: int

    create(x: int) -> self: obj {
        self.x = x
    }
}

fn main() {
    let a = obj(7)
    var? p = a
    if (p != nil) {
        print("%i", p.x)
    }
}

/// [Test]
/// name = "nilcheck/conditional/if"
/// info = "Nil check narrows a type"

/// [Expects]
/// success = true
/// output = "7"
